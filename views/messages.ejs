<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p><a href="/messages/new">Create new message</a></p>
    <div class="messages">
        <% messages.forEach((message) => { %>
            <div class="message">
                <p><%= message.user %> </p>
                 <p><%= message.text %> </p>
                 <p><%= message.added %> </p>
            </div>
            
        <% })  %> 
        
    </div>
  </body>
</html>
